doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title #{tittle}
    body
        h1 Users
        div#users
        
        div 
            each user in users
                div.user
                    p.name #{user.nombre}
            